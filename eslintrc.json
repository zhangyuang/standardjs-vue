{
  "extends": [
    "plugin:vue/vue3-recommended",
    "standard",
    "standard-with-typescript"
  ],
  "rules": {
    "vue/max-attributes-per-line": ["error", {
      "singleline": 8,
      "multiline": {
        "max": 8,
        "allowFirstLine": true
      }
    }],
    "@typescript-eslint/explicit-function-return-type": "off",
    "@typescript-eslint/strict-boolean-expressions": "off"
  }
}
